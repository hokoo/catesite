    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "johnpbloch/wordpress": "^6",
    "composer/installers": "^1.0@dev",
    "mobiledetect/mobiledetectlib": "^2.8",
    "wpackagist-plugin/classic-editor": "^1.5",
    "wpackagist-plugin/wp-smtp": "^2",
    "johnbillion/query-monitor": "^3.7",
    "wpackagist-plugin/wp-data-logger": "^2.0",
    "wpackagist-theme/twentytwenty": "^2.0",
    "wpackagist-plugin/image-compare-block": "^1",
    "wpackagist-plugin/amazon-s3-and-cloudfront": "^3"
  },
  "autoload": {
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ]
    }
  },
  "scripts": {
    "pre-install-cmd": [],
    "post-install-cmd": []
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
